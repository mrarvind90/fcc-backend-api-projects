{
  "name": "fcc-backend-api-projects",
  "version": "0.9.0",
  "description": "A backend API microservice built as a project for freeCodeCamps' Backend Development and API course certification",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node -r dotenv/config --watch src/server.js dotenv_config_path=.env.dev",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write --config .prettierrc.json",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrarvind90/fcc-backend-api-projects.git"
  },
  "keywords": [
    "freeCodeCamp",
    "javascript",
    "mongodb",
    "nodejs",
    "expressjs",
    "express-validator",
    "cors",
    "mongoose",
    "multer"
  ],
  "author": "Arvindran Kalidas <arvindrankalidas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrarvind90/fcc-backend-api-projects/issues"
  },
  "homepage": "https://github.com/mrarvind90/fcc-backend-api-projects#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "mongoose": "^7.5.2",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  }
}
